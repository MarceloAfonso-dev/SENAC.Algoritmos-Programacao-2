programa {
  funcao inicio() {
    escrevalS("Olá, mundo!")
    escrevalI(42)
    escrevalD(3.14)
  }

  funcao escrevalS(cadeia texto) {
    escreva(texto)
    escreva("\n")
  }

  funcao escrevalI(inteiro numero) {
    escreva(numero)
    escreva("\n")
  }

  funcao escrevalD(real numeroReal) {
    escreva(numeroReal)
    escreva("\n")
  }
}
